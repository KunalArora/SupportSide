<div class = 'consumable'>
  <dl>
    <dt>Consumables</dt>
  <dd>
  <% if consumable.present? %>
    <% shown_status = false %>
    <% shown_life = false %>
    <% consumable.each do |c| %>
      <% consumables(c['object_id'], parser(c)).try(:each)  do |k,v| %>
        <% if !k.nil? %>
          <% if c['object_id']=='1.3.6.1.4.1.2435.2.3.9.4.2.1.5.5.8.0' %>
          <dl>
          <% k.each do |l, m|%>
            <% unless shown_status %>
              <h3>Status:</h3>
              <% shown_status=true %>
            <% end %>
            <span style='font-weight: bold; padding: 5px;'><%= l %>:</span> <%= m %>
          <% end %>
          <% v.each do |r, s|%>
            <% unless shown_life %>
              <h3 style ='float: right; margin-right: 50%; margin-top: -50px'>Remaining Life:</h3>
              <% shown_life=true %>
            <% end %>
            <span style ='float: right; margin-right: 50%'><span style='font-weight: bold; padding: 5px;'><%= r %>:</span><%= s %></span>
          <% end %>
        </dl>
          <% else %>
           <dl>
            <span style='font-weight: bold; padding: 5px'><%= k %>:</span><%= v %>,
          </dl>
          <% end %>
        <% end %>
      <% end %>

  <!--    <dd>( since <%= c.updated_date %> )</dd> -->
    <% end %>
  <% else %>
    -
  <% end %>
</dd></dl>
</div>
